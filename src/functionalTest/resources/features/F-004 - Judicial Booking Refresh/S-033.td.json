{
  "_guid_": "S-033",
  "title": "must successfully create judicial role mappings for User having no judicial booking",
  "_extends_": "F-004_Test_Data_Base",

  "specs": [
    "a judicial profile that supports judicial bookings",

    "contains the actorId of the user having existing judicial role assignments",
    "contains the actorId of the user for which no bookings exist",

    "contains a bookable fee-paid-judge role-assignment",
    "contains no booked judge/fee-paid role-assignments"
  ],

  "users": {
    "invokingUser": {
      "_extends_": "AmJRDUser2"
    }
  },

  "request": {
    "body": {
      "actorId": ["${[scenarioContext][testData][users][invokingUser][id]}"],
      "roleName" : ["judge", "fee-paid-judge"],
      "attributes": {
        "contractType": ["Fee-Paid"]
      }
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body" : {
      "roleAssignmentResponse" : [
        {
          "__elementId__": "roleName,attributes.jurisdiction,attributes.region"
        },
        {
          "id": "[[ANYTHING_PRESENT]]",
          "actorIdType": "IDAM",
          "actorId": "${[scenarioContext][testData][request][body][actorId][0]}",
          "roleType": "ORGANISATION",
          "roleName": "fee-paid-judge",
          "classification": "PUBLIC",
          "grantType": "STANDARD",
          "roleCategory": "JUDICIAL",
          "readOnly": false,
          "beginTime": "[[ANYTHING_PRESENT]]",
          "endTime" : "[[ANYTHING_PRESENT]]",
          "created": "[[ANYTHING_PRESENT]]",
          "attributes": {
            "bookable" : "true",
            "substantive": "N",
            "contractType": "Fee-Paid",
            "jurisdiction": "SSCS",
            "region" : "2",
            "primaryLocation": "[[ANYTHING_PRESENT]]",
            "workTypes": "pre_hearing,hearing_work,post_hearing,decision_making_work,routine_work,priority"
          },
          "authorisations" : "[[ANYTHING_PRESENT]]"
        }
      ]
    }
  }
}
