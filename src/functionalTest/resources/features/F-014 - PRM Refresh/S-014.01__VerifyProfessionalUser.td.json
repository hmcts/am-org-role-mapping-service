{
  "_guid_": "S-014.01__VerifyProfessionalUser",
  "_extends_": "FetchProfessionalUsersById__PositiveResponse_Base",

  "specs": [
    "to verify professional user has userAccessTypes enabled"
  ],

  "request": {
    "queryParams": {
      "userId": "${[scenarioContext][parentContext][testData][request][queryParams][userId]}"
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body": {
      "users": [
        {
          "_extends_": "ProfessionalUser",
          "organisationInfo": {
            "status": "ACTIVE",
            "organisationProfileIds" : [ "SOLICITOR_PROFILE" ]
          },
          "userAccessTypes": [ {
            "jurisdictionId" : "[[ANYTHING_PRESENT]]",
            "organisationProfileId" : "SOLICITOR_PROFILE",
            "accessTypeId" : "[[ANYTHING_PRESENT]]",
            "enabled" : true
          } ]
        }
      ]
    }
  }

}
