          "id": "${[scenarioContext][parentContext][childContexts][S-010_DeleteDataForRoleAssignments01][testData][request][body][userIds][0]}",

          "case_allocator": "Y",
          "task_supervisor": "Y",

          "role": [
            {
              "_extends_": "CaseworkerRole_01_SLC"
            }
          ],

          "work_area": [
            {
              "_extends_": "WorkArea_BFA1_IA"
            }
          ],
          "base_location": [
            {
              "created_time": "[[ANY_TIMESTAMP_NOT_NULLABLE]]",
              "last_updated_time": "[[ANY_TIMESTAMP_NOT_NULLABLE]]",
              "location_id": "[[ANY_INTEGER_NOT_NULLABLE]]",
              "location": "[[ANYTHING_PRESENT]]",
              "is_primary": false
            }
          ]
        }
      ]
    }
  }

}
