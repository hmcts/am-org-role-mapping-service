package validationrules.core;

import java.util.List;

function String getJurisdictionFromServiceCode(String serviceCode) {
    String result = null;
    if (serviceCode == "BBA3") {
       result = "SSCS";
    } else if (List.of("AAA6", "AAA7").contains(serviceCode)) {
       result = "CIVIL";
    }
    return result;
}